{{page-title "Registration"}}

{{#if this.session.isAuthenticated}}
  <Jumbo @hideHamster="true" @title="My account">
    My profile: <LinkTo @route="person" @model={{this.person}}>
      {{this.person.name}}
    </LinkTo><br>
    <br>
    <Auth::Logout />
  </Jumbo>
{{else}}
  <Auth::Login />
  <Auth::Register />
{{/if}}